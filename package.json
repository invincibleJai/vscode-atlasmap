{
	"name": "atlasmap-viewer",
	"description": "Opens a browser for Atlas Data Mapper",
	"version": "0.0.1",
	"publisher": "jboss-fuse",
	"preview": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/jboss-fuse/vscode-atlasmap.git"
	},
	"license": "Apache-2.0",
	"engines": {
		"vscode": "^1.29.1"
	},
	"categories": [
		"Other"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "atlasmap.open",
				"title": "Open Atlas Map in default system browser",
				"category": "Atlas Map"
			},
			{
				"command": "atlasmap.start",
				"title": "Start Atlas Map",
				"category": "Atlas Map"
			}
		]
	},
	"activationEvents": [
		"onCommand:atlasmap.open",
		"onCommand:atlasmap.start"
	],
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test",
		"tslint": "gulp tslint"
	},
	"dependencies": {
		"chunk": "^0.0.2",
		"fetch": "^1.1.0",
		"opn": "^5.4.0",
		"vscode": "^1.1.26"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.5",
		"@types/node": "^10.12.18",
		"mocha": "^5.2.0",
		"tslint": "^5.12.0",
		"typescript": "^3.2.2"
	}
}
